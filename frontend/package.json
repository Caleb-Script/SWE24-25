{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_EXTRA_CA_CERTS=./certificates/server.crt next dev -p 3002",
    "dev:tls": "next --experimental-https",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "eslint": "eslint ./",
    "eslint:9": "set ESLINT_USE_FLAT_CONFIG=false&& eslint src __tests__",
    "eslint:debug": "eslint --debug src __tests__",
    "prebuild": "npm run clean",
    "pre:dev": "npm run copy-resources",
    "debug": "nest start --debug --watch",
    "test:ORIG": "jest",
    "pretest": "npm run copy-resources",
    "test": "set LOG_LEVEL=info&& jest --runInBand",
    "test:istanbul": "npm t -- --coverage",
    "test:logdebug": "jest --runInBand",
    "test:dbserver": "set LOG_LEVEL=info&& set START_DB_SERVER=true&&jest --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "asciidoctor": "node scripts/asciidoctor",
    "clean": "rimraf dist log/server.log",
    "invoke-webrequest": "pwsh -Command Invoke-WebRequest https://localhost:3000/rest/1 -SslProtocol Tls13 -SkipCertificateCheck",
    "prettier": "prettier --cache --write \"./**/*.ts\" \"./**/*.tsx\"",
    "prettier:check": "prettier --cache --check \"./**/*.ts\" \"./**/*.tsx\"",
    "typedoc": "typedoc",
    "tsc": "tsc --pretty"
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@heroicons/react": "^2.1.3",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "formik": "^2.4.6",
    "graphql-request": "^7.0.1",
    "http-proxy-middleware": "^3.0.0",
    "js-cookie": "^3.0.5",
    "lint": "^1.1.2",
    "next": "14.2.4",
    "next-auth": "^4.24.7",
    "react": "^18",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^18",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.52.0",
    "sharp": "^0.33.4",
    "use-debounce": "^10.0.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cross-env": "7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "prettier": "^3.3.2",
    "typescript": "^5"
  }
}
